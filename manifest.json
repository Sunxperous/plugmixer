{
  "manifest_version": 2,

  "name": "Plugmixer",
  "description": "Overcome the 200 item count limit in plug.dj playlists!",
  "version": "0.12",

  "permissions": [
    "http://plug.dj/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://plug.dj/*"],
      "js": ["contentscript.js"]
    }
  ],
  "web_accessible_resources": [
    "mix.js"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}