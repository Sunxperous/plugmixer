{
  "manifest_version": 2,

  "name": "Plugmixer",
  "description": "Overcome the 200 item count limit in plug.dj playlists!",
  "version": "0.1",

  "permissions": [
    "http://plug.dj/"
  ],
  "content_scripts": [
    {
      "matches": ["http://plug.dj/*"],
      "js": ["inject.js"]
    }
  ],
  "web_accessible_resources": [
    "mix.js"
  ]
}